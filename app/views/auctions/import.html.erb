 <h2>Import Auctions</h2>
 <%= form_tag import_auctions_path, multipart: true, class: 'form-inline' do %>
  <div class="form-group">
    <%= file_field_tag :file, class: '', required: true %>
  </div>
  
  <div class="form-group">
    <%= submit_tag "Import CSV", class: 'btn btn-info' %>
  </div>
<% end %>